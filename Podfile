source 'https://github.com/CocoaPods/Specs.git'
  
platform :ios, '12.0'
use_frameworks!

target 'Airside' do
  pod 'SnapKit'
  pod 'Alamofire' 
  pod 'ObjectMapper'
  pod 'AlamofireObjectMapper', '~> 5.2.0'
  pod 'SwiftyBeaver'
  pod 'Kingfisher'
  pod 'IQKeyboardManager'
  pod 'Keyboard+LayoutGuide'

  abstract_target 'Tests' do
    target 'AirsideTests'

    pod 'Quick'
    pod 'Nimble'
  end
end


